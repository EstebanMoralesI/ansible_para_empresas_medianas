hostname {{ inventory_hostname }}
!
enable password {{ ansible_password }}
!
ip domain-name cisco.com
!
{% for item dns_servers %}
ip name-server {{ item }}
{% endfor %}
!
line vty 0 4
 login local
 transport input ssh 
!
line vty 5 15
 login local
 transport input ssh
!
{% for ntp in ntp_servers %}
ntp server {{ ntp }}
{% endfor %}
